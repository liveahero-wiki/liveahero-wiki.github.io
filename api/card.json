---
layout: none
---
{%- assign array = "" | split: "," %}
{%- for pair in site.data.SidekickMaster %}
{%- assign unit = pair[1] %}
{%- if unit.levelZone > 1 %}{%- continue %}{%- endif %}
{%- assign page = unit.characterId | characterIdToPage %}
{%- capture info %}{"cardName":"{{ unit.stockId | stockIdToCharaTitle: 1 }}","resourceName":"{{ unit.resourceName}}","rarity":{{ unit.rarity }},
"type":"{{ page.type }}","gender":"{{ page.gender }}"}{% endcapture %}
{%- assign array = array | push: info %}
{%- endfor %}
[{{ array | join: "," }}]